<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>editUser</from-outcome>
			<to-view-id>/editUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewUser</from-outcome>
			<to-view-id>/viewUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listUsers</from-outcome>
			<to-view-id>/listUsers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>createUser</from-outcome>
			<to-view-id>/createUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
